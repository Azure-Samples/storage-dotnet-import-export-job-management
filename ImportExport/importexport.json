                    "url": "https://msdn.microsoft.com/en-us/library/dn529104.aspx"
                },
                "parameters": [
                    {
                        "name": "originlocation",
                        "in": "query",
                        "description": "The location for which alternate locations are being queried. If this parameter is not specified, all locations will be returned.",
                        "required": false,
                        "type": "string"
                    },
                    {"$ref": "#/parameters/subscriptionId"},
                    {"$ref": "#/parameters/Accept-Language"},
                    {"$ref": "#/parameters/Accept"},
                    {"$ref": "#/parameters/x-ms-version"}                    
                ],
                "responses": {
                    "200": { 
                        "schema": {
                            "$ref": "#/definitions/ListLocationsResponse"
                        }
                    },
                    "default": {
                        "description": "An error occurs.",
                        "schema": {
                            "$ref": "#/definitions/ErrorResponse"
                        }
                    }
                }
            }
        },
